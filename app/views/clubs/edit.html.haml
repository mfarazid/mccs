.col-xs-12.col-sm-9
  %section.box.box-default
    .box-header
      %h3.box-title Editing club      
    .box-body  
      = render 'form'
    .box-footer
      %span= link_to '<i class="fa fa-arrow-circle-left"></i>'.html_safe, clubs_path, :class => "btn btn-default", :title => "Back to clubs"
      %span= link_to '<i class="fa fa-eye"></i>'.html_safe, @club, :class => "btn bg-maroon pull-right", :title => "View this club" 
  %br
= render 'sidenavbar'  