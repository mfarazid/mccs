.row
  .col-sm-6
    = bootstrap_form_for @venue do |f|
      - if @venue.errors.any?
        #error_explanation
          %h2= "#{pluralize(@venue.errors.count, "error")} prohibited this venue from being saved:"
          %ul
            - @venue.errors.full_messages.each do |msg|
              %li= msg

      = f.text_field :name
      = f.text_field :address
      = f.text_field :city
      = f.text_field :county_or_state
      = f.text_field :country
      = f.submit 'Save'
